package com.demo.test;

import com.demo.beans.Employee;

public class TestSinglyLinkedListEmployee {
	public static void main(String[] args) {
		singlyLinkedListEmployee elist=new singlyLinkedListEmployee();
		elist.addNode(new Employee(12,"sai","2373833"));
		elist.addNode(new Employee(13,"Anjali","458998"));
		elist.addNode(new Employee(14,"sharvari","355333"));
		elist.addNode(new Employee(15,"sujata","586553"));
		elist.addNode(new Employee(16,"shreyas","456783"));
		elist.displayData();
		
		System.out.println("----------------------------");
		elist.addNodeByEmpno(new Employee(17,"sharvari","355333"),14);
		elist.displayData();
		
		System.out.println("----------------------------");
		
		elist.deleteNodeByEmpno(16);
		elist.deleteNodeByEmpno(12);
		elist.deleteNodeByEmpno(17);
		elist.deleteNodeByEmpno(20);
		elist.displayData();
	}

}
