package com.demo.beans;

import com.demo.beans.DoublyLinkedList.Node;

public class CircularLinkedList {
	Node head;
	 class node{
		int data;
		Node next;
		
	 node( int value){
		 this.data = value;
		 this.next =null;
	 }
	
		 public void addNode(int value) {
	
			 Node newNode = new Node(value);
				if(head == null) {
					head = newNode;
					newNode.next = newNode;
					Node temp = head;
					while(temp.next != head) {
						temp = temp.next;
					}
					temp.next = newNode;
					newNode.next=head;
					}
					}
		 public void displayData() {
			 if(head == null) {
				 
			 }
				 
		 }
				}
}
			 
			


			 
		 
