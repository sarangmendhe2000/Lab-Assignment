package com.demo.test;

import java.util.Scanner;

import com.demo.array.SingleDimension;

public class TestSingleDArray {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the size of the array");
		int n = sc.nextInt();
		
		int[] arr = new int[n];
		
		SingleDimension obj = new SingleDimension();
		obj.acceptData(arr);
		obj.display(arr);

	}

}
