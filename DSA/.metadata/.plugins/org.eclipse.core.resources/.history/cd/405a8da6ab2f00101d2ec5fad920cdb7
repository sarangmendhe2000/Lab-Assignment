package com.demo.test;

import com.demo.beans.Employee;

public class singlyLinkedListEmployee {
		Node head;
		class Node{
			Employee data;
			Node next;
			
			public Node(Employee data) {
				super();
				this.data=data;
				this.next=null;
			}
			
			
			
			public void addNodeByEmployee(Employee ob,int empno) {
				
				if(head == null) {
					System.out.println("List is Empty!");
				}else {
					Node newNode = new Node(ob);
					Node temp = head;
					while(temp.next != null && temp.data.getEmpno()!=empno) {
						temp = temp.next;
					}
					
					if(temp.data.getEmpno() == empno) {
						temp.next=newNode;
					}else {
						System.out.println("empno not found!" + empno);
					}
				}
				
			}
			
		}
		public void addNode(Employee ob) {
			// TODO Auto-generated method stub
			Node newNode = new Node(ob);
			if(head == null) {
				head = newNode ;
				
			}else {
				Node temp = head;
				while(temp.next != null) {
					temp=temp.next;
				}
				temp.next=newNode;
			}
			
		}
	

	}


