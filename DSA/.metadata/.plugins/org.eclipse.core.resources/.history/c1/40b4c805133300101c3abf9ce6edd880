import java.util.Scanner;
public class FibonacciSeries {
	
	public static void main(String[] args, String a) {
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter number:");
		int a = 0, b=1;
		System.out.println("num:"+a);
		int n = sc.nextInt();
		
		for(int i=1;i<=n;i++) {
			
			int result = a+b;
			a = b;
			b = result;
		}
		sc.close();	
		
	}

}
