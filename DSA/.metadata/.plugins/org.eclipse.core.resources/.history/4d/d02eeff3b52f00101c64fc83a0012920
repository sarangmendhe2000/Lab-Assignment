package com.demo.beans;

public class DoublyLinkedList {
	Node head;
	     class Node{
		int data;
		Node next;
		Node prev;
		
	public Node(int data){
			this.data=data;
			this.next=null;
			this.prev=null;
		}
	}
	public void insertAtBeginning(int data){
		
	Node newNode = new Node(data);
		if(head == null) {
			head = newNode;
			return;	
		}
	newNode.next = head;
	head.prev = newNode;
	head = newNode;	
	}
	/*
	 public void insertAtEnd(int data, Node temp) {
		 Node newNode= new Node(data);
		 if( head == null) {
			head= newNode;
			return;
			
		 }
		 
		 while(temp.next!= null) {
			 temp = temp.next; 
		 }
		 
		 temp.next = newNode;
		 newNode.prev= temp;
		 
	 }
	 
}
