package com.demo.test;
import com.demo.beans.MyStack;

public class MyTest {
	
    public MyStack stack = new MyStack();

    public static void main(String[] args) {
        MyStack myStack = new MyStack();
        myStack.Stack.push(10);
        myStack.Stack.push(20);
        myStack.stack.push(30);
        myStack.stack.DisplayData();
        myStack.stack.pop();
        myStack.stack.DisplayData();
        System.out.println("Top element: " + myStack.stack.peek());
	
	
	

}
