package com.demo.service;


public class SinglyLinkList {
    
	Node head;
	public class Node{
		int data;
		Node next;
		
		Node(int x)
		{
			this.data=x;
			this.next = null;
		}
		
	}
	
	
	public SinglyLinkList()
	{
		head = null;
	}
	
	// Add new Node 
	
	public static void addNewNode(int x)
	{
		Node newNode = new Node();
	}
	
	  
}
