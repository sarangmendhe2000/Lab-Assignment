package com.demo.beans;

public class DoublyCircularLinkedList {

	Node head;
	class Node{
		int data;
		Node next;
		Node prev;
		//constructor
		public Node(int val) {
			this.data=val;
			this.next=null;
			this.prev=null;
		}
	}
	public void addnode(int value) {
		Node newNode = new Node (value);
		if(head == null) {
			newNode.next = newNode;
			newNode.prev = newNode;
			
		}
	}
	
	
}
