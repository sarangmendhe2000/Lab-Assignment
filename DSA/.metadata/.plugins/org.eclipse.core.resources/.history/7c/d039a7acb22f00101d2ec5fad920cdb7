package com.demo.test;

import com.demo.beans.Employee;

public class TestSinglyLinkedListEmployee {

	public static void main(String[] args) {
		
		singlyLinkedListEmployee elist = new singlyLinkedListEmployee();
		System.out.println("-----------------------------------------------");
		System.out.println("Employee Object");
		elist.addNode(new Employee(1,"sai","2345677"));
		elist.addNode(new Employee(2,"om","12344"));
		elist.addNode(new Employee(3,"ritu","12344"));
		elist.addNode(new Employee(4,"rohini","12344"));
		elist.addNode(new Employee(2,"ram","12344"));
		elist.addNode(new Employee(2,"nikhil","12344"));
		elist.displayData();
		System.out.println("-----------------------------------------------");
		
		elist.deleteNodeByEmpno(3);
		elist.deleteNodeByEmpno(1);
		
		elist.displayData();
		
	

	}

}
