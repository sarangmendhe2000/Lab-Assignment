package com.demo.test;

import com.demo.beans.MyBinarySearchTree;

public class TestBinarySearch {
    public static void main(String[] args) {
    	
      MyBinarySearchTree ob = new MyBinarySearchTree();
      int[] arr = {10,5,4,8,3,7,18,45,6 };
      for(int i= 0; i<arr.length; i++) {
    	  
    	  ob.insertNode(arr[i]);	  
      }
   	
      ob.inorder();
      System.out.println("--------------------------------");
      ob.preorder();
      System.out.println("--------------------------------");
      
       System.out.println("found : 18"+ob.search(10));
		System.out.println("found : 17"+ob.search(17));
		System.out.println("found : 7"+ob.searchNonRecurssive(10));
		System.out.println("found : 45"+ob.searchNonRecurssive(17));
}

}