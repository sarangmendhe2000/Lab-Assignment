package com.demo.service;

import java.util.Set;

import com.demo.beans.Employee;

public interface EmployeeService {
	
	boolean addNewEmployee();

	Set<Employee> displayAll();

	Employee displayById(int id);

	Set<Employee> searchByName(String name);

	Set<Employee> searchBySalary(double sal);

	boolean updateBySalary(int uid, double sall);

	boolean deleteById(int del_id);

	boolean deleteBySalary(double del_sal);

	Set<Employee> sortById();


}
