package com.demo.MySpringbootRestWebService.Dao;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.demo.MySpringbootRestWebService.Bean.Student;

@Repository
public interface StudentDao extends JpaRepository<Student, Integer> {
	
	@Query(Value="Select * from student3 where sid between :lr and :hr" , nativeQuery = true);
	List<Student> findByRange(int lr, int hr);
	
	
}
